server.port=8040

#service discovery url
eureka.client.serviceUrl.defaultZone=http://localhost:9001/eureka
#service name
spring.application.name=cloud-telecom-app

#my_private_key
private.key=${private.key}

#dbs
telecom.driver=${telecom.driver}
telecom.url=${telecom.url}
telecom.username=${telecom.username}
telecom.password=${telecom.password}
telecom.initialSize=${telecom.initialSize}
telecom.maxActive=${telecom.maxActive}
telecom.minIdle=${telecom.minIdle}
telecom.maxWait=${telecom.maxWait}
telecom.defaultAutoCommit=${telecom.defaultAutoCommit}
telecom.testWhileIdle=${telecom.testWhileIdle}
telecom.testOnBorrow=${telecom.testOnBorrow}
telecom.testOnReturn=${telecom.testOnReturn}
telecom.timeBetweenEvictionRunsMillis=${telecom.timeBetweenEvictionRunsMillis}
telecom.minEvictableIdleTimeMillis=${telecom.minEvictableIdleTimeMillis}
telecom.validationQuery=${telecom.validationQuery}
telecom.removeAbandoned=${telecom.removeAbandoned}
telecom.removeAbandonedTimeout=${telecom.removeAbandonedTimeout}
telecom.logAbandoned=${telecom.logAbandoned}
        